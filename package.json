{
  "name": "bankly-transfer-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start": "node -r module-alias/register -r dotenv/config dist/app/entrypoint.js",
    "start-dev": "NODE_ENV=development tsnd --transpile-only --ignore-watch node_modules -r tsconfig-paths/register -r dotenv/config src/app/entrypoint.ts",
    "compose-dev:up": "docker-compose --env-file .env up -d",
    "compose-dev:config": "docker-compose --env-file .env config",
    "typeorm-dev": "tsnd -r tsconfig-paths/register -r dotenv/config node_modules/typeorm/cli.js",
    "migration-dev:generate": "npm run typeorm-dev -- migration:generate -d src/infra/typeorm/ormconfig.typeorm.ts src/infra/typeorm/migrations/migration",
    "migration-dev:revert": "npm run typeorm-dev -- migration:revert -d src/infra/typeorm/ormconfig.typeorm.ts",
    "migration-dev:run": "npm run typeorm-dev -- migration:run -d src/infra/typeorm/ormconfig.typeorm.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@config": "dist/app/config",
    "@domain": "dist/domain",
    "@infra": "dist/infra",
    "@interface": "dist/interface",
    "@shared": "dist/shared"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "metadata": "^0.1.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.4",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "validate": "^5.2.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
